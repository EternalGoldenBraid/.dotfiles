    "# Visualize tangent line to a conic section\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the conic section\n",
    "a = 1; b = 0; c = 1; d = 0; e = 0; f = -1\n",
    "\n",
    "# Define the point\n",
    "x0 = 0; y0 = 0\n",
    "\n",
    "# Define the tangent line\n",
    "m = 1; b = 0\n",
    "\n",
    "# Define the grid\n",
    "x = np.linspace(-2, 2, 100)\n",
    "y = np.linspace(-2, 2, 100)\n",
    "X, Y = np.meshgrid(x, y)\n",
    "\n",
    "# Define the conic section in matrix form\n",
    "C = np.array([[a, b/2, d/2], [b/2, c, e/2], [d/2, e/2, f]])\n",
    "\n",
    "# Define the point in matrix form\n",
    "P = np.array([[x0], [y0], [1]])\n",
    "\n",
    "# Define the tangent line in matrix form\n",
    "T = np.array([[m, -1, 0], [-1, 0, 0], [0, 0, 0]])\n",
    "\n",
    "# Compute the conic section\n",
    "Z = C[0,0]*X**2 + C[0,1]*X*Y + C[0,2]*Y**2 + C[1,1]*X*Y + C[1,2]*Y**2 + C[2,2]*X**2 + C[2,2]*Y**2 + C[2,2]\n",
    "\n",
    "# Compute the tangent line\n",
    "Zt = T[0,0]*X**2 + T[0,1]*X*Y + T[0,2]*Y**2 + T[1,1]*X*Y + T[1,2]*Y**2 + T[2,2]*X**2 + T[2,2]*Y**2 + T[2,2]\n",
    "\n",
    "# Plot the conic section\n",
    "plt.figure()\n",
    "fig, axs = plt.subplots(1, 2, figsize=(10, 5))\n",
    "axs[0].contour(X, Y, Z, [0], colors='k')\n",
    "# axs[0].plot(x0, y0, 'ro')\n",
    "# axs[0].plot(x, m*x + b, 'b')\n",
    "# axs[0].plot(x, -x + y0 + m*x0, 'g')\n"
   ]
